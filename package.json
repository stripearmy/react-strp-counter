{
  "name": "react-strp-counter",
  "author": "Arsen Mkrtchyan (stripearmy)",
  "private": false,
  "version": "1.1.1",
  "description": "A lightweight counter that relies on CSS to rearrange numbers and doesn't rerender Elements",
  "repository": "https://github.com/stripearmy/react-strp-counter.git",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/react-strp-counter.esm.js",
      "require": "./dist/react-strp-counter.js",
      "types": "./dist/types/index.d.ts"
    },
    "./css": "./dist/css/index.js",
    "./styles.css": "./dist/css/styles.css"
  },
  "main": "dist/react-strp-counter.js",
  "module": "dist/react-strp-counter.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npx rollup -c",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "rollup": "^3.29.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "react",
    "counter",
    "digit",
    "number",
    "css counter",
    "number counter",
    "number animation",
    "counter animation",
    "digit animation",
    "casino counter",
    "casino digits",
    "casino numbers",
    "jackpot counter",
    "jackpot numbers",
    "jackpot digits",
    "flip",
    "flip counter",
    "flip numbers",
    "flip digits",
    "odometer",
    "css odometer",
    "react odomteter"
  ],
  "license": "MIT"
}
